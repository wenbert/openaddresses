{
    "coverage": {
        "country": "us",
        "state": "mi",
        "county": "Livingston, Macomb, Monroe, Oakland, Saint Clair, Washtenaw, Wayne"
    },
    "data": "http://arcgisserver.semcog.org/arcgis/rest/services/OpenData/Buildings/MapServer/0",
    "type": "ESRI",
    "conform": {
        "number": "LOADD1",
        "street": "STREET1",
        "unit": {
            "function": "regexp",
            "field": "STREET1",
            "pattern": "(\\b(?:UNIT|APT|SUITE|BLDG|LOT|#)\\b.+)$",
            "replace": "$1"
        },
        "postcode": "ZIPCODE",
        "type": "geojson"
    }
}
